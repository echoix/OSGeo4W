-	$cmd .= " -h sha256";
-	$cmd .= " -i \"https://qgis.org\"";
-	$cmd .= " -t \"http://timestamp.digicert.com\"";
-	$cmd .= " -in \"$base.exe\"";
-	$cmd .= " $base-signed.exe";
-	system $cmd;
-	die "signing failed [$cmd]" if $?;
-
-	rename("$base-signed.exe", "$base.exe") or die "rename failed: $!";
