-if exist "%PROGRAMFILES%\CMake\bin" path %PATH%;%PROGRAMFILES%\CMake\bin
-if exist "%PF86%\CMake\bin" path %PATH%;%PF86%\CMake\bin
-if exist c:\cygwin64\bin path %PATH%;c:\cygwin64\bin
