-%TAR% -C %OSGEO4W_ROOT% -cjf %ARCH%/release/qgis/%PACKAGENAME%/%PACKAGENAME%-%VERSION%-%PACKAGE%.tar.bz2 ^
-	--exclude-from exclude ^
-	--exclude "*.pyc" ^
-	apps/%PACKAGENAME% ^
-	bin/%PACKAGENAME%-bin.exe ^
-	bin/%PACKAGENAME%-bin.vars ^
