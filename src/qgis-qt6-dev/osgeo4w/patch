-setlocal enabledelayedexpansion
-
-set VERSION=%1
-set PACKAGE=%2
-set PACKAGENAME=%3
-set ARCH=%4
-set SHA=%5
-set SITE=%6
-if "%VERSION%"=="" goto usage
-if "%PACKAGE%"=="" goto usage
-if "%PACKAGENAME%"=="" goto usage
-if "%ARCH%"=="" goto usage
-if not "%SHA%"=="" set SHA=-%SHA%
